{
    "llm": {
        "provider": "openai-compatible",
        "model": "gpt-4o-mini",
        "baseUrl": "https://api.openai.com/v1",
        "apiKeyEnv": "OPENAI_API_KEY",
        "timeoutMs": 60000,
        "jsonMode": true
    },
    "web": {
        "search": {
            "enabled": true,
            "provider": "browser",
            "maxResults": 5,
            "headless": false
        },
        "fetch": {
            "enabled": true,
            "maxChars": 30000,
            "timeoutSeconds": 15
        }
    }
}